{
  "name": "deribit-ws-js",
  "version": "3.0.3",
  "description": "Deribit.com WebSocket browser generic library",
  "main": "./lib/index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "babel src -d lib",
    "lint": "eslint src",
    "prepare": "yarn build",
    "precommit": "yarn lint",
    "prepush": "yarn test"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "allowImportExportEverywhere": false,
      "codeFrame": false,
      "impliedStrict": true
    },
    "globals": {
      "use": true
    },
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3"
  },
  "repository": "git@github.com:santacruz123/deribit-ws-js.git",
  "author": "Andrey Vorobyov <vorandrew@gmail.com>",
  "private": false
}
